{
	"name": "@napkin-ide/forge",
	"version": "0.0.15",
	"scripts": {
		"build": "npm run build:all",
		"build:all": "npm run build:common && concurrently \"npm:build:public\" \"npm:build:ide\" \"npm:build:infra\" \"npm:build:org-reg\" \"npm:build:settings\"",
		"build:common": "ng build common",
		"build:ide": "ng build ide --prod",
		"build:infra": "ng build infra --prod",
		"build:public": "ng build public --prod",
		"build:settings": "ng build settings --prod",
		"build:org-reg": "ng build org-reg --prod",
		"deploy-man": "npm run deploy && npm run git:clean",
		"deploy": "npm run versions && npm run build && npm run deploy:all",
		"deploy:all": "concurrently \"npm:deploy:common\" \"npm:deploy:public\" \"npm:deploy:ide\" \"npm:deploy:infra\" \"npm:deploy:org-reg\" \"npm:deploy:settings\"",
		"deploy:common": "npm publish ./dist/common --access public",
		"deploy:ide": "npm publish ./dist/ide --access public",
		"deploy:infra": "npm publish ./dist/infra --access public",
		"deploy:org-reg": "npm publish ./dist/org-reg --access public",
		"deploy:public": "npm publish ./dist/public --access public",
		"deploy:settings": "npm publish ./dist/settings --access public",
		"git:clean": "git add . && git commit -a -m \"Changes during manual deploy\"",
		"versions": "npm version patch && npm run version:all",
		"version:all": "concurrently \"npm:version:common\" \"npm:version:public\" \"npm:version:ide\" \"npm:version:settings\" \"npm:version:infra\" \"npm:version:org-reg\"",
		"version:common": "npm version patch --prefix projects/common",
		"version:ide": "npm version patch --prefix projects/ide",
		"version:public": "npm version patch --prefix projects/public",
		"version:settings": "npm version patch --prefix projects/settings",
		"version:infra": "npm version patch --prefix projects/infra",
		"version:org-reg": "npm version patch --prefix projects/org-reg",
		"ng": "ng",
		"start": "ng serve",
		"start:public": "ng serve public --port=4211",
		"start:infra": "ng serve infra --port=4242",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"module": "ng g module ___ --project=___",
		"comp": "ng g component ___ --project=___ -m ___"
	},
	"private": false,
	"dependencies": {
		"@angular/animations": "~8.1.2",
		"@angular/cdk": "^8.1.1",
		"@angular/common": "~8.1.2",
		"@angular/compiler": "~8.1.2",
		"@angular/core": "~8.1.2",
		"@angular/flex-layout": "^7.0.0-beta.24",
		"@angular/forms": "^8.1.2",
		"@angular/http": "^7.2.7",
		"@angular/material": "^8.1.1",
		"@angular/platform-browser": "~8.1.2",
		"@angular/platform-browser-dynamic": "~8.1.2",
		"@angular/router": "~8.1.2",
		"@aspnet/signalr": "^1.1.4",
		"@lcu-ide/common": "^0.15.589",
		"@lcu-ide/lcu-identity-common": "^0.3.216-angular8",
		"@lowcodeunit-devkit/ng-lcu": "0.0.167",
		"@lowcodeunit/lazy-element": "0.0.8",
		"angular-oauth2-oidc": "^5.0.2",
		"core-js": "^2.5.4",
		"dot-object": "^1.7.1",
		"hammerjs": "^2.0.8",
		"rxjs": "~6.4.0",
		"tslib": "^1.9.0",
		"zone.js": "~0.8.29"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^0.801.2",
		"@angular-devkit/build-ng-packagr": "^0.801.2",
		"@angular/cli": "^8.1.2",
		"@angular/compiler-cli": "~8.1.2",
		"@angular/language-service": "~8.1.2",
		"@lcu/cli": "^0.1.25-integration",
		"@types/jasmine": "~2.8.8",
		"@types/jasminewd2": "~2.0.3",
		"@types/node": "~8.9.4",
		"codelyzer": "^5.1.0",
		"concurrently": "^4.1.0",
		"jasmine-core": "~2.99.1",
		"jasmine-spec-reporter": "~4.2.1",
		"karma": "~3.1.1",
		"karma-chrome-launcher": "~2.2.0",
		"karma-coverage-istanbul-reporter": "~2.0.1",
		"karma-jasmine": "~1.1.2",
		"karma-jasmine-html-reporter": "^0.2.2",
		"ng-packagr": "^5.3.0",
		"node-sass": "^4.13.1",
		"protractor": "~5.4.0",
		"ts-node": "~7.0.0",
		"tsickle": ">=0.34.0",
		"tslint": "~5.11.0",
		"typescript": "^3.4.5"
	}
}
